module Operators {
    import Std.IO.*;

    function main(args: String[]): int {
        var i: int = 5;
        println(i);
        
        i += 10; println(i);
        i -= 5; println(i);
        i /= 2; println(i);
        i *= 3; println(i);

        i = i + 10; println(i);
        i = i - 5; println(i);
        i = i / 2; println(i);
        i = i * 3; println(i);

        i++; println(i);
        i--; println(i);

        println(i % 5);
        println(i % 31);
        return 0;
    }
}
