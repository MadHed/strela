module SumTypes {
    import Std.IO.*;

    function printSum(sum: int | String) {
        match(sum) {
            case String println(sum);
            case int println(sum);
        }
    }

    function main(args: String[]): int {

        var sum: String | int;

        sum = 1337;
        printSum(sum);

        sum = "Hello";
        printSum(sum);

        return 0;
    }
}