module Generics {
    import Std.IO.*;
    
    class Foo<T> {
        var val: T;
        function display() {
            print("Value is: ");
            println(this.val);
        }

        function init(v: T) {
            print("init() called: ");
            println(v);
            this.val = v;
        }
    }

    function main(args: String[]): i32 {
        var s = new Foo<String>("String");
        var i = new Foo<i32>(123);
        var f = new Foo<f32>(1.5);

        s.display();
        i.display();
        f.display();
        return 0;
    }
}